{
  "name": "loxia-autopilot-cli",
  "version": "0.6.1",
  "description": "Loxia Labs Multipurpose AI-Agent - CLI and VS Code Extension (Obfuscated)",
  "main": "index.js",
  "type": "module",
  "bin": {
    "loxia": "./index.js"
  },
  "extensionEntry": "extension.js",
  "author": "Daniel Suissa",
  "license": "SEE LICENSE IN LICENSE.txt",
  "publisher": "loxia-labs",
  "displayName": "Loxia AI Agent",
  "categories": [
    "AI",
    "Code Generation",
    "Productivity"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "commands": [
      {
        "command": "loxia.chat",
        "title": "Start AI Chat",
        "category": "Loxia"
      },
      {
        "command": "loxia.analyze",
        "title": "Analyze Code",
        "category": "Loxia"
      }
    ],
    "views": {
      "explorer": [
        {
          "id": "loxiaChat",
          "name": "Loxia AI",
          "when": "true"
        }
      ]
    }
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.40.1",
    "@azure-rest/ai-inference": "^1.0.0-beta.6",
    "@azure-rest/ai-vision-image-analysis": "^1.0.0-beta.3",
    "@google/generative-ai": "^0.24.1",
    "boxen": "^7.1.1",
    "cfonts": "^3.3.0",
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "express-rate-limit": "^7.5.1",
    "figlet": "^1.8.1",
    "helmet": "^8.1.0",
    "inquirer": "^12.6.0",
    "openai": "^4.97.0",
    "ora": "^8.2.0",
    "puppeteer": "^24.8.0",
    "sequelize": "^6.37.7",
    "sqlite3": "^5.1.7",
    "table": "^6.8.1",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "dotenv": "^16.5.0",
    "node-fetch": "^3.3.0",
    "axios": "^1.7.0",
    "@modelcontextprotocol/sdk": "^1.0.0",
    "express": "^5.1.0"
  },
  "engines": {
    "node": ">=16.0.0",
    "vscode": "^1.80.0"
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "echo 'No TypeScript compilation needed'",
    "package:extension": "npx @vscode/vsce package --no-dependencies --out loxia-autopilot.vsix",
    "package:npm": "npm pack",
    "package:all": "npm run package:npm && npm run package:extension",
    "deploy:extension": "npx @vscode/vsce publish",
    "deploy:npm": "npm publish",
    "test:cli": "node index.js --version",
    "test:extension": "echo 'Extension test requires VS Code environment'",
    "dev": "node index.js"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/loxia-labs/autopilot-cli.git"
  },
  "homepage": "https://github.com/loxia-labs/autopilot-cli",
  "bugs": {
    "url": "https://github.com/loxia-labs/autopilot-cli/issues"
  },
  "keywords": [
    "ai",
    "automation",
    "development",
    "assistant",
    "code-generation"
  ]
}